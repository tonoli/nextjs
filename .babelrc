{
  "presets": [
    ["next/babel"]
  ],
  "plugins": [
    "inline-react-svg",
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "extensions": [
          ".js", ".ts", ".jsx", ".tsx", "d.ts"
        ],
        "alias": {
          "@src": "./src",
          "@components": "./src/components"
        },
        "compilerOptions": {
          "baseUrl": ".",
          "paths": {
            "@src": ["./src"],
            "@components/*": ["./src/components/*"]
          }
        }
      }
    ]
  ]
}
